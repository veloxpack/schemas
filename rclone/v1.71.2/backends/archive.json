{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.veloxpack.io/rclone/v1.71.2/backends/archive.json",
  "title": "Archive Reader",
  "description": "Read ZIP and SquashFS archives",
  "type": "object",
  "properties": {
    "remote": {
      "type": "string",
      "title": "Remote",
      "description": "Remote to wrap to read archives from. Can be 'myremote:path/to/dir', 'myremote:bucket' or empty to use root",
      "examples": [
        "myremote:",
        "myremote:path"
      ]
    }
  },
  "required": [],
  "additionalProperties": false
}

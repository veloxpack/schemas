{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.veloxpack.io/rclone/v1.71.2/backends/quatrix.json",
  "title": "Quatrix",
  "description": "Quatrix by Maytech business file sharing",
  "type": "object",
  "required": [
    "api_key",
    "host"
  ],
  "properties": {
    "api_key": {
      "type": "string",
      "title": "API Key",
      "description": "API key for accessing Quatrix account",
      "format": "password"
    },
    "host": {
      "type": "string",
      "title": "Host",
      "description": "Host name of Quatrix account",
      "examples": [
        "mycompany.quatrix.it"
      ]
    },
    "hard_delete": {
      "type": "boolean",
      "title": "Hard Delete",
      "description": "Delete files permanently rather than putting them into the trash",
      "default": false
    },
    "skip_project_folders": {
      "type": "boolean",
      "title": "Skip Project Folders",
      "description": "Skip project folders in operations",
      "default": false
    },
    "encoding": {
      "type": "string",
      "title": "Encoding",
      "description": "The encoding for the backend (advanced)",
      "default": "Slash,BackSlash,Del,Ctl,InvalidUtf8,Dot"
    }
  },
  "additionalProperties": false
}

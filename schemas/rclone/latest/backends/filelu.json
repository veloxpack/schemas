{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.veloxpack.io/schemas/rclone/latest/backends/filelu.json",
  "title": "FileLu",
  "description": "FileLu Cloud Storage",
  "type": "object",
  "required": [
    "key"
  ],
  "properties": {
    "key": {
      "type": "string",
      "title": "Rclone Key",
      "description": "Your FileLu Rclone key from My Account",
      "format": "password"
    },
    "encoding": {
      "type": "string",
      "title": "Encoding",
      "description": "The encoding for the backend (advanced)",
      "default": "Slash,LtGt,DoubleQuote,SingleQuote,BackQuote,Question,Dollar,Colon,Asterisk,Pipe,Hash,Percent,BackSlash,CrLf,Del,Ctl,LeftSpace,LeftPeriod,LeftTilde,LeftCrLfHtVt,RightPeriod,RightCrLfHtVt,RightSpace,Semicolon,Exclamation,SquareBracket,InvalidUtf8,Dot"
    }
  },
  "additionalProperties": false
}

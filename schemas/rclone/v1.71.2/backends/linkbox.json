{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.veloxpack.io/schemas/rclone/v1.71.2/backends/linkbox.json",
  "title": "LinkBox",
  "description": "LinkBox file storage",
  "type": "object",
  "required": ["token"],
  "properties": {
    "token": {
      "type": "string",
      "title": "Token",
      "description": "Token from https://www.linkbox.to/admin/account",
      "format": "password"
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "token": "your-linkbox-token"
    }
  ]
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.veloxpack.io/schemas/rclone/v1.71.2/backends/uptobox.json",
  "title": "Uptobox",
  "description": "Uptobox file hosting service",
  "type": "object",
  "properties": {
    "access_token": {
      "type": "string",
      "title": "Access Token",
      "description": "Your access token. Get it from https://uptobox.com/my_account",
      "format": "password"
    },
    "private": {
      "type": "boolean",
      "title": "Private Files",
      "description": "Set to make uploaded files private",
      "default": false
    },
    "encoding": {
      "type": "string",
      "title": "Encoding",
      "description": "The encoding for the backend (advanced)",
      "default": "Slash,LtGt,DoubleQuote,BackQuote,Del,Ctl,LeftSpace,InvalidUtf8,Dot"
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "access_token": "your-access-token"
    },
    {
      "access_token": "your-access-token",
      "private": true
    }
  ]
}
